{
  "name": "realt",
  "version": "1.0.2-rc.11",
  "description": "Realt is a new way to work with Redux inspired by Alt",
  "browser": "dist/redux.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "lint": "eslint src",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=development rollup -c -i src/index.js -o dist/realt.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -i src/index.js -o dist/realt.min.js",
    "build": "npm run build:commonjs && npm run build:es &&npm run build:umd && npm run build:umd:min"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Vnkitaev/realt.git"
  },
  "keywords": [
    "realt",
    "altjs",
    "state",
    "redux",
    "redux-alt",
    "redux-altjs",
    "redux-actions",
    "redux-reducers",
    "redux-actions-class"
  ],
  "files": [
    "dist",
    "es",
    "lib",
    "src"
  ],
  "author": {
    "name": "Ivan Kitaev",
    "email": "vnkitayev@gmail.com"
  },
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "reduce-reducers": "^0.1.2"
  },
  "devDependencies": {
    "cross-env": "^3.2.3",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "eslint": "^3.17.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-config-airbnb-base": "^11.1.1"
  }
}